{
  "projects": {
    "app_marketing_analytics_config": null,
    "ama_adwords_adapter": [ "google-bqdts" ],
    "ama_fb_adapter": [  "stitchdata", "fivetran" ]
  },
  ""
  "vendors": [
    {
      "id": "google-adwords",
      "name": "Google Adwords",
      "url": "https://adwords.google.com/home"
    },
    {
      "id": "google-bqdts",
      "name": "Google BigQuery Data Transfer Service",
      "url": "https://cloud.google.com/bigquery",
      "gitUrl": "https://github.com/looker/ama_adwords_bqdts_adapter.git"
    },
    {
      "id": "facebook",
      "name": "Facebook Ads",
      "url": "https://www.facebook.com/business/products/ads",
      "optional": true
    },
    {
      "id": "stitchdata",
      "name": "Facebook Ads ETL by Stitch",
      "url": "https://www.stitchdata.com/integrations/facebook-ads",
      "gitUrl": "https://github.com/looker/ama_fb_stitch_adapter.git",
      "optional": true
    },
    {
      "id": "fivetran",
      "name": "Facebook Ad Insights by Fivetran",
      "url": "https://fivetran.com/directory/facebook_ads_insights",
      "gitUrl": "https://github.com/looker/ama_fb_fivetran_adapter.git",
      "optional": true
    }
  ],
  "steps": [
    {
      "name": "Install Google Adwords BigQuery Adapter",
      "type": "createProject",
      "data": {
        "project": "ama_adwords_adapter"
      }
    },
    {
      "name": "Create Config Project",
      "type": "createBareProject",
      "data": {
        "project": "app_marketing_analytics_config"
      }
    },
    {
      "name": "Add Self to Embed Domain Whitelist",
      "type": "addSelfToEmbedDomainWhitelist",
      "data": {}
    },
    {
      "name": "Activate Project Import Feature",
      "type": "setLabsFeatureValue",
      "data": {
        "featureName": "project_import",
        "value": true
      }
    },
    {
      "name": "Google AdWords",
      "type": [ "writeConfigView", "enableModule" ],
      "module": "search",
      "data": {
        "fields": [
          {
            "name": "adwords_schema",
            "label": "Schema Name"
          },
          {
            "name": "adwords_customer_id",
            "label": "Customer ID"
          }
        ]
      }
    },
    {
      "name": "Install Facebook Adapter",
      "type": [ "writeConfigView", "createProject", "enableModule" ],
      "optional": true,
      "module": "social",
      "data": {
        "project": "ama_fb_adapter",
        "data": {
          "fields": [
            {
              "name": "facebook_etl",
              "label": "ETL Provider",
              "type": "chooseProjectSource"
            },
            {
              "name": "facebook_schema",
              "label": "Schema (Database) Name"
            }
          ]
        }
      }
    },
    {
      "name": "Test Database Connection",
      "type": "testDbConnection"
    },
    {
      "name": "Test Model Configuration",
      "type": "testModelConfig"
    }
  ]
}
